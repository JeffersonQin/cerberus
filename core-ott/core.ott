grammar
 binop :: 'Op' ::= 
%   (* arithmetic operators *)
   | +     :: :: Add    
   | -     :: :: Sub    
   | *     :: :: Mul    
   | /     :: :: Div    
   | rem_t :: :: Rem_t  
%(* this the % of C ===> rem_t(x,y) = x - ((truncate(x/y) * y) *)
   | rem_f :: :: Rem_f 
%(* rem_t(x,y) = x - ((foor(x/y) * y) *)
   | ^     :: :: Exp
%   (* relational operators *)
   | =     :: :: Eq
   | >     :: :: Gt
   | <     :: :: Lt
   | >=    :: :: Ge
   | <=    :: :: Le
%   (* logical connectives *)
   | /\    :: :: And 
   | \/    :: :: Or




