-}
fun main () : [integer] :=
  let strong x = create(<alignof>("signed int"), "signed int") in
  let strong _ = par(
    let strong _ = store("signed int", x, 0) in
    return (unit)
    end
  ,
    let strong _ = store("signed int", x, 1) in
    return (unit)
    end
  ) in
  let strong _ = kill(x) in
  return(0)
  end end end
