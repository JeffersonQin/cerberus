open Global

type ordering = Lt | Eq | Gt

class (Ord 'a)
  val compare : 'a -> 'a -> ordering
end




let to_int = function
 | Lt -> Int.neg (Int.int 1)
 | Eq -> Int.int 0
 | Gt -> Int.int 1
 end

let of_int i =
  if i = (Int.int 0) then
    Eq
  else if Int.(>=) i (Int.int 1)
    then Gt
    else Lt

