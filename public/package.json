{
  "name": "Cerberus",
  "version": "1.0.0",
  "description": "Cerberus UI",
  "private": true,
  "scripts": {
    "start": "webpack --config dev.config.js -w",
    "build": "webpack --config dev.config.js",
    "deploy": "webpack --config prod.config.js",
    "test": "jest",
    "report": "nyc report --reporter=html"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/tests/*.+(ts|tsx|js)"
    ],
    "testResultsProcessor": "jest-jenkins-reporter"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rems-project/cerberus-private"
  },
  "homepage": "https://www.cl.cam.ac.uk/~pes20/cerberus/",
  "keywords": [],
  "author": "Victor Gomes",
  "license": "MIT",
  "devDependencies": {
    "@types/codemirror": "0.0.56",
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.1",
    "@types/lodash": "^4.14.107",
    "@types/vis": "^4.21.3",
    "css-loader": "^0.28.11",
    "jasmine-reporters": "^2.3.1",
    "jest": "^22.4.3",
    "jest-jenkins-reporter": "^1.0.2",
    "nyc": "^11.7.1",
    "puppeteer-to-istanbul": "^1.2.2",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@types/puppeteer": "^1.2.2",
    "codemirror": "^5.36.0",
    "golden-layout": "^1.5.9",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "npm": "^5.8.0",
    "puppeteer": "^1.3.0",
    "vis": "^4.21.0"
  }
}
