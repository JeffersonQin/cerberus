let unit = state`unit
let bind = state`bind
let map  = state`map

let update f = fun s -> ((), f s)
let modify f = fun s -> f s
let read   f = fun s -> (f s, s)

let create size v = list`create size v
let init   size f = list`init   size f

let get i = List`nth i

