let annotate_program A (startup, S) =
  match Context.lookup eq_identifier S.declarations startup with
    | Just (Decl_function ret_ty _ (* bs *) _ (* is_variadic *) _ (* is_inline *) _ (* is_Noreturn *)) ->
        (* TODO: check arguments *)
        if ret_ty = Basic (Integer (Signed Int_)) then
          annotate_sigma A S >>= fun S ->
          E.return (startup, S)
        else
          E.fail (TError_TODO 1067)
    | _ ->
        E.fail (TError_TODO 67) (* TODO: this is not realy a type error, but more an csem internal one. *)
  end
