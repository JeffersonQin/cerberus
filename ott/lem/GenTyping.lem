let annotate_rvalue_aux A annotate_expression e =
  annotate_expression e >>= fun e ->
  match type_of A e with
    | GenRValueType gty ->
        E.return (e, rvalue_coercion gty)
    | GenLValueType _ ty ->
        AilTypesAux.lvalue_coercion ty >>= fun ty' ->
        E.return (e, inject_type ty')
  end


(* TODO: check if something needs to done regarding the basis *)
let type_of_constant ic =
  match ic with
  | (n, _, Nothing) ->
      if AilTypesAux.in_min_integer_range n (Signed Int_) then
        Concrete (Signed Int_)
      else
        Unknown ic
  | (n, _, Just U) ->
      if AilTypesAux.in_min_integer_range n (Unsigned Int_) then
        Concrete (Unsigned Int_)
      else
        Unknown ic
  | (n, _, Just L) ->
      if AilTypesAux.in_min_integer_range n (Signed Long) then
        Concrete (Signed Long)
      else
        Unknown ic
  | (n, _, Just UL) ->
      if AilTypesAux.in_min_integer_range n (Unsigned Long) then
        Concrete (Unsigned Long)
      else
        Unknown ic
  | (_, _, Just LL) ->
      Concrete (Signed LongLong)
  | (_, _, Just ULL) ->
      Concrete (Unsigned LongLong)
end


let well_typed_assignment t1 gt2 null2 =
  match (t1, gt2) with
    | (Pointer q1 t1, GenPointer q2 t2) ->
           null2
        || AilTypesAux.sub_qualifiers q2 q1 && (   AilTypesAux.compatible t1 t2
                                                || AilTypesAux.is_void t1 && AilTypesAux.is_object t2
                                                || AilTypesAux.is_void t2 && AilTypesAux.is_object t1)
    | (Pointer _  _, _) ->
        null2
    | (_, GenPointer _  _) ->
        AilTypesAux.is_bool t1
    | (_, _) ->
        AilTypesAux.is_arithmetic t1 && is_arithmetic gt2
  end


(* STD §6.5.9#2 *)
val well_typed_equality: genType -> genType -> bool -> bool -> bool
let well_typed_equality gty1 gty2 is_null1 is_null2 =
     is_arithmetic gty1 && is_arithmetic gty2         (* STD §6.5.9#2, item 1 *)
  || pointers_to_compatible_types gty1 gty2           (* STD §6.5.9#2, item 2 *)
  || pointer_to_void gty1   && pointer_to_object gty2 (* STD §6.5.9#2, item 3 *)
  || pointer_to_object gty1 && pointer_to_void gty2   (* STD §6.5.9#2, item 3 *)
  || is_pointer gty1 && is_null2                      (* STD §6.5.9#2, item 4 *)
  || is_null1        && is_pointer gty2               (* STD §6.5.9#2, item 4 *)


let well_typed_binary_arithmetic gt1 aop gt2 : bool =
  match aop with
    | Mul  -> is_arithmetic gt1 && is_arithmetic gt2
    | Div  -> is_arithmetic gt1 && is_arithmetic gt2
    | Mod  -> is_integer    gt1 && is_integer    gt2
    | Add  -> is_arithmetic gt1 && is_arithmetic gt2
    | Sub  -> is_arithmetic gt1 && is_arithmetic gt2
    | Shl  -> is_integer    gt1 && is_integer    gt2
    | Shr  -> is_integer    gt1 && is_integer    gt2
    | Band -> is_integer    gt1 && is_integer    gt2
    | Bxor -> is_integer    gt1 && is_integer    gt2
    | Bor  -> is_integer    gt1 && is_integer    gt2
  end


let combine_qualifiers_left gt1 gt2 : genType =
  match (gt1, gt2) with
    | (GenPointer q1 t1, GenPointer q2 _) -> GenPointer (AilTypesAux.combine_qualifiers q1 q2) t1
    | (GenPointer _  _ , _              ) -> gt1
    | (_               , _              ) -> gt1
  end


let combine_qualifiers_right gt1 gt2 : genType =
  match (gt1, gt2) with
    | (GenPointer q1 _, GenPointer q2 t2) -> GenPointer (AilTypesAux.combine_qualifiers q1 q2) t2
    | (_              , GenPointer _   _) -> gt2
    | (_              , _               ) -> gt2
  end


let well_typed_conditional gt1 gt2 gt3 null2 null3 : E.t genTypeCategory =
  if is_scalar gt1 then
    if is_arithmetic gt2 && is_arithmetic gt3 then
      match usual_arithmetic gt2 gt3 with
        | Just gt -> E.return (GenRValueType gt)
        | Nothing -> E.fail (TError_TODO 8)
      end
    else
      match composite_pointer gt2 gt3 with
        | Just gt -> E.return (GenRValueType gt)
        | Nothing -> if void gt2 && void gt3 then
                       E.return (GenRValueType GenVoid)
                     else if is_pointer gt2 && null3 then
                       E.return (GenRValueType (combine_qualifiers_left gt2 gt3))
                     else if is_pointer gt3 && null2 then
                       E.return (GenRValueType (combine_qualifiers_right gt2 gt3))
                     else if pointer_to_object gt2 && pointer_to_void gt3 then
                       E.return (GenRValueType (combine_qualifiers_right gt2 gt3))
                     else if pointer_to_object gt3 && pointer_to_void gt2 then
                       E.return (GenRValueType (combine_qualifiers_left gt2 gt3))
                     else E.fail (TError_TODO 9)
      end
  else
    E.fail (TError_TODO 10)


let annotate_assignee_aux (annotate_rvalue : expression 'A1 -> E.t (expression 'A2 * genType)) =
  fun t1 e2 ->
    annotate_rvalue e2 >>= fun (e2, gt2) ->
    if well_typed_assignment t1 gt2 (null_pointer_constant e2)
      then E.return e2
      else E.fail (TError_TODO 11)


