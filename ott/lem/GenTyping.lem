let rec annotate_arguments_aux A annotate_expression annotate_assignee is_variadic es p =
  match (es, p) with
    | ([], []) ->
        E.return []
    | ([], [(_, Void)]) ->
        E.return []
    | (e :: es, (_, t1) :: p) ->
        annotate_assignee (AilTypesAux.rvalue_coercion t1) e                            >>= fun e  ->
        annotate_arguments_aux A annotate_expression annotate_assignee is_variadic es p >>= fun es ->
        E.return (e :: es)
    | (_, []) ->
        (* This case must correspond to a call to a variadic function *)
        if not is_variadic then
          E.fail (TError_TODO 68)
        else
          (* following (§6.5.2.2#7) *)
          (* TODO: check *)
          E.mapM annotate_expression es
    | (_, _) ->
        E.fail (TError_TODO 12)
  end
















let annotate_stringLiteral (pref_opt, str) =
  let elem_ty = match pref_opt with
                  | Nothing     -> Basic (Integer Char)     (* STD §6.4.5#6 sentence 3 *)
                  | Just Enc_u8 -> Basic (Integer Char)     (* STD §6.4.5#6 sentence 4 *)
                  | Just Enc_u  -> Basic (Integer Char16_t) (* STD §6.4.5#6 sentence 6 *)
                  | Just Enc_U  -> Basic (Integer Char32_t) (* STD §6.4.5#6 sentence 6 *)
                  | Just Enc_L  -> Basic (Integer Wchar_t)  (* STD §6.4.5#6 sentence 5 *)
                end in

(* TODO: check the type *)
  E.return (AilEstr (pref_opt, str),
          GenRValueType (GenArray elem_ty (integerFromNatural (naturalFromNat (List.length (String.toCharList str) + 1))))
         )




let rec annotate_constant cst =
  match cst with
    | ConstantInteger ic ->
        E.return (ConstantInteger ic, GenRValueType (GenBasic (GenInteger (type_of_constant ic))))
    | ConstantCharacter cc ->
        (* TODO: §6.4.4.4#9 *)
        match fst cc with
          | Nothing ->
              (* §6.4.4.4#10 *)
              E.return (ConstantCharacter cc, GenRValueType (GenBasic (GenInteger (Concrete (Signed Int_)))))
          | Just Pref_L ->
              (* §6.4.4.4#11 *)
              E.return (ConstantCharacter cc, GenRValueType (GenBasic (GenInteger (Concrete Wchar_t))))
          | Just Pref_u ->
              (* §6.4.4.4#11 *)
              E.return (ConstantCharacter cc, GenRValueType (GenBasic (GenInteger (Concrete Char16_t))))
          | Just Pref_U ->
              (* §6.4.4.4#11 *)
              E.return (ConstantCharacter cc, GenRValueType (GenBasic (GenInteger (Concrete Char32_t))))
        end
    
    | ConstantArray csts ->
        E.mapM annotate_constant csts >>= fun a_csts ->
        (* TODO: this is probably too strong *)
        match a_csts with
          | [] ->
              E.fail (TError "empty array constant")
          | (_, genTy)::a_csts' ->
              if List.all (fun (_, genTy') -> genTy = genTy') a_csts' then
                E.return (ConstantArray (List.map fst a_csts),
                          GenRValueType (GenArray (* TODO *) Void (integerFromNat $ List.length a_csts)))
              else
                E.fail (TError "array constant elements don't all have the same type")
        end
  end


