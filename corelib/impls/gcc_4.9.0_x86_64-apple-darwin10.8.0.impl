-- GCC uses the two's complement representation with no padding bits
fun <Integer.encode> (ty: ctype, n: integer) : integer :=
  encodeTwos(ty, n)

fun <Integer.decode> (ty: ctype, n: integer) : integer :=
  decodeTwos(ty, n)


